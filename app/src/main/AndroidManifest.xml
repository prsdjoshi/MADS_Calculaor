<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calcmads">

    <application
        android:name="com.example.calcmads.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.calcmads.calc.HistoryActivity"
            android:theme="@style/DashTheme"></activity>
        <activity
            android:name="com.example.calcmads.ui.login.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/DashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.calcmads.calc.CalcActivity"
            android:theme="@style/DashTheme" />
    </application>

</manifest>